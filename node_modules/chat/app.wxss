/*去除默认样式*/

button {
  position: relative;
  display: block;
  margin-left: 0;
  margin-right: auto;
  padding-left: 0;
  padding-right: 0;
  box-sizing: border-box;
  font-size: 0;
  text-align: center;
  text-decoration: none;
  line-height: 0;
  border-radius: 0;
  -webkit-tap-highlight-color: #fff;
  overflow: hidden;
  color: #fff;
  background-color: #fff;
}
button::after {
  content: "";
  width: 100%;
  height: 200%;
  position: absolute;
  top: 0;
  left: 0;
  border: 0px;
  -webkit-transform: scale(0);
  transform: scale(0);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  box-sizing: border-box;
  border-radius: 0px;
}

