/* components/title/index.wxss */
.a-title{
  width: 750rpx;
  height: 93rpx;
  line-height: 93rpx;
  box-sizing: border-box;
  padding-left: 31rpx;
  position: relative;
}
.a-title::before{
  content: '';
  position: absolute;
  left:0rpx;
  margin-top: 30rpx;
  width: 10rpx;
  height: 36rpx;
  background: #80CFAF;
}
.titlebg::before{
  content: '';
  left:-1000rpx;
}