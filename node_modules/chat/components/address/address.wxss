/* components/address/address.wxss */
 .address-list{
    padding-top: 20rpx;
}

 .address-list .address-item{
    height: 106rpx;
    line-height: 106rpx;
    padding-left: 30rpx;
    font-size: 28rpx;
    color: #343434;
    border-bottom: 1rpx solid #f2f2f2;
}

.address-list .address-item radio{
    transform: scale(0.7) translateY(-4rpx);
}

.btn-block{
    width: 690rpx;
    height: 84rpx;
    border-radius: 42rpx;
    margin: auto;
    color: #fff;
    font-size: 30rpx;
    line-height: 84rpx;
    text-align: center;
    background: #38D092;
  }