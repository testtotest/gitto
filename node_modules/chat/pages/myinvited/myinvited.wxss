.go-detail {
  margin: 0 auto;
  width: 690rpx;
  height: 100rpx;
  line-height: 100rpx;
  background: rgba(246, 247, 249, 1);
  border-radius: 12rpx;
  box-sizing: border-box;
  padding-left: 30rpx;
  padding-right: 30rpx;
}
.go-detail Image {
  width: 20rpx;
  height: 20rpx;
}
.go-detail-left {
  font-size: 30rpx;
  font-weight: 400;
  color: rgba(52, 52, 52, 1);
}
.go-detail-right {
  font-size: 26rpx;
  font-weight: 400;
  color: rgba(154, 154, 154, 1);
}

.title {
  margin-top: 20rpx;
}

.margin-bottom {
  width: 750rpx;
  height: 70rpx;
}
