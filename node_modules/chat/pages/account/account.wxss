page{
  background: #F6F7F9;
}
.user-icon{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 21rpx;
}
.user-icon>image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.user-name{
  color: #333333;
  font-size: 36rpx;
}
.login-c{
  height: 132rpx;
  background: #fff;
  box-sizing: border-box;
  padding:0 30rpx !important;
  color: #38CF91;
  font-size: 32rpx;
}
.unlogin-c{
  box-sizing: border-box;
  margin: 0 auto;
  text-align: center;
}

.more-btn{
  font-size:24rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.more-btn >image{
  width: 20rpx;
  height: 20rpx;
  margin-left: 10rpx;
}
.moneydet{
  height: 120rpx;
  background-color: #fff;
}
.moneydet-item{
  flex: 1;
  text-align: center;
}
.moneydet-item .count{
  font-size: 40rpx;
  color: #333333;
  font-weight: 500;
}
.moneydet-item .title{
  font-size: 24rpx;
  color: #333333;
}

.nav-c{
  margin: 30rpx auto;
  width: 690rpx;
}
.nav-c>.item-nav{
  font-size: 0;
}
.nav-c>.item-nav>image{
  width: 334rpx;
  height: 148rpx;
}
.nav{
  background-color: #fff;
  width: 690rpx;
  height: 172rpx;
  margin: 0 auto;
  padding: 30rpx 0;
  box-sizing: border-box;
  margin-top: 20rpx;
  border-radius: 12rpx;
}
.nav .nav-item{
  flex: 1;
  color: #333;
  font-size: 26rpx;
  text-align: center;
}

.nav image{
  width: 48rpx;
  height: 48rpx;
  display: block;
  margin:0 auto 18rpx;
}

.form-c{
  width: 690rpx;
  margin-top: 20rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  border-radius: 12rpx;
}
.form-item{
  width: auto;
  border: none;
}

.item-form-title{
  display: flex;
  align-items: center;
}