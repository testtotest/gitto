<view class="d-j">
	<view class="w-50 st1"></view>
	<view class="w-50 st2"></view>	
</view>
<view class="w-100 bg-f mt-10">	
	    <view class="item-input-t item-input-t-border">
			<view class="item-input-t-l">地区选择</view>
			<view class="item-input-t-m" bindtap="onRegionTap">
				<input model:value="{{area}}" placeholder="请选择" placeholder-class="placeholder" disabled="true" />
			</view>			
		</view>
		<view class="item-input-t">
			<view class="item-input-t-l">地区选择</view>
			<view class="item-input-t-m">
				<input type="text" style="width: 100%;font-size: 14px;color:#666666;" model:value="{{address}}" placeholder="例：xxx居委会(村) xxx街道路 xx小区1号"/>
			</view>	 
		</view>	
</view>
<view class="w-100 bg-f mt-10">	
    <view class="item-input-t item-input-t-border" bindtap="onBtna">
    	<view class="item-inputs-t-l">是否住宅用电</view>
    	<view class="item-inputs-t-m">{{isshowd}}</view>
     	<view class="item-inputs-t-r"><image src="/images/gengduo@2x.png" style="width: 30rpx;height: 30rpx;"></image></view>
    </view>
	<view class="item-input-t item-input-t-border">
		<view class="item-inputs-t-l">申请容量(选填)</view>
		<view class="item-inputs-t-m" style="width: 60%;" style="padding: 0;">
			<input type="text" style="width: 100%;font-size: 14px;color:#666666;" model:value="{{testa}}" placeholder="0-160之间"/>
			<span style="color: #666666;font-size: 11.5px;">kVA</span>
		</view>	 			
	</view>
	<view  class="picup">
		   <view class="picup-title">
		   	 		  <label class="picup-title-lable">企业主体证明照片</label>
		   	 		  <label class="picup-title-lables">最多可上传6张照片</label>
		   </view>
					    <view class="picup-list">
							<view wx:for="{{imgz}}" wx:for-item="item" wx:key="*this" class="picup-list-l">
							<image src="{{item.photo}}" style="width: 100%;height: 60px;"/>
							</view>
							<view class="picup-but" bindtap="choosezImg">
											    <view class="picup-but-pic">
													<image src="/images/xiangji@2x.png"/>
												</view>
												<view class="picup-but-btn">点击拍摄/上传</view>
							</view>	 		   
						</view>
	</view>		
</view>
<view style="position:fixed; bottom:0;width:100%;height:300px;background-color:#fff;border-top-left-radius: 6px;border-top-right-radius: 6px;z-index: 5;" wx:if="{{showd}}">	
 	  <view style="padding: 15px;">
 		  <view style="display: flex;justify-content:flex-end;">
 		  		<view style="width:70%;font-size: 14.5px;color:#343434">是否是住宅用电</view>
 		  		<view bindtap="cloBtnb">
 					<image src="/images/close.png" style="width: 14px;height: 14px;"/>
 				</view>
 		  </view>
 		  <view style="font-size: 16.5px;line-height: 40px;color:#343434;" wx:for="{{itemsshowd}}" id='{{index}}' bindtap="selBtnb">
 			  {{item.message}}
 		  </view>
 	  </view>	 
</view>

<view class="w-100 bg-f mt-10">	
     <view class="item-input-t item-input-t-border" >
     	<view class="item-inputs-t-l">服务预约时间</view>
     	<view class="item-inputs-t-m">
			
		    <picker mode="date" start="" end="" value='' fields="day" bindchange='changeDate'>
		     <input value="{{currentDate}}" disabled="true" placeholder='请选择服务预约时间' />
		     </picker>	
		</view>
      	<view class="item-inputs-t-r"><image src="/images/gengduo@2x.png" style="width: 30rpx;height: 30rpx;"></image></view>
     </view>
</view>
<view class="bt1">
	<button type="button" class="btn" bindtap="btnForm">下一步</button>
</view>
	 
<view catchtap="hideCover"  class='bg' wx:if="{{isbg}}"></view>
<view class="model-public-c" wx:if="{{showAddressModel}}">
    <view class='address-c animated {{showAddressModel ?"fadeInUp":""}}'>
        <image bindtap="closedModel" class="closed-btn" src="../../images/close.png" />
        <view class="address-title">请选择</view>
        <view class="address-tab">
            <block wx:for="{{areaArray}}">
                <text>{{item}}</text>
            </block>
        </view>
        <scroll-view class="address-list" scroll-y="true" style="height:700rpx;">   
            <block wx:for="{{addressView}}" wx:key="index">
                <view class="address-item" data-id="{{item.id}}" data-parentId="{{item.parentId}}" data-name="{{item.name}}" bindtap="onRegionTap"> 
                    {{item.name}}
                </view>
            </block>
        </scroll-view>
    </view>
</view>
