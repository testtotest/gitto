<view class="d-j">
		<view class="w-30 st2"></view>
		<view class="w-30 st2"></view>	
		<view class="w-30 st1"></view>
</view>

<view class="w-100 bg-f mt-10">
	<view class="item-input-t item-input-t-border" bindtap="onBtna">
		<view class="item-inputs-t-l">法人证件类型</view>
		<view class="item-inputs-t-m">{{isshowd}}</view>
	 	<view class="item-inputs-t-r"><image src="/images/gengduo@2x.png" style="width: 30rpx;height: 30rpx;"></image></view>
	</view>
	<view  class="picup" wx:if="{{showpicaModal}}">
		   <view class="picup-title">
		   	 		  <label class="picup-title-lable">法人身份证件照</label>
		   	 		  <label class="picup-title-lables">最多可上传6张照片</label>
		   </view>
					    <view class="picup-list">
							<view wx:for="{{imgzz}}" wx:for-item="item" wx:key="*this" class="picup-list-l">
							<image src="{{item.photo}}" style="width: 100%;height: 67px;"/>
							</view>
							<view class="picup-but" bindtap="choosezImg">
											    <view class="picup-but-pic">
													<image src="/images/xiangji@2x.png"/>
												</view>
												<view class="picup-but-btn">点击拍摄/上传</view>
							</view>	 		   
						</view>
	</view>	
		<view  class="picup" wx:if="{{showpicbModal}}">
			   <view class="picup-title">
			   	 		  <label class="picup-title-lable">身份证件照片</label>
			   	 		  <label class="picup-title-lables">最多可上传2张照片</label>
			   </view>
						    <view class="picup-listp">
								<view  bindtap="chooseiaImg" class="picup-listp-l" style="background: url({{iaa}});background-repeat:no-repeat;background-size:100% 70px;margin-right: 10px;padding-top: 3px;" >
								    <view class="picup-but-pic" style="margin-top: 6px;">
								    	<image src="/images/xiangji@2x.png"/>
								    </view>
								    <view class="picup-but-btn">点击拍摄/上传</view>
								</view>
									<view bindtap="chooseibImg"  class="picup-listp-l" style="margin-right: 10px;background: url({{ibb}});background-repeat:no-repeat;background-size:100% 70px;padding-top: 3px;">
									    <view class="picup-but-pic" style="margin-top: 6px;">
									    	<image src="/images/xiangji@2x.png"/>
									    </view>
									    <view class="picup-but-btn">点击拍摄/上传</view>
									</view>							
							</view>
		</view>		
</view>	
<view class="w-100 bg-f mt-10 mb-25">	
     <view class="item-input-t item-input-t-border">
     	<view class="item-input-t-l">姓名</view>
     	<view class="item-input-t-m">
     		<input type="text" model:value="{{ownerName}}" style="width: 100%;font-size: 14px;color:#666666;"  placeholder="请输入姓名"/>
     	</view>	 
     </view>
	<view class="item-input-t item-input-t-border">
		<view class="item-input-t-l">身份证件号码</view>
		<view class="item-input-t-m">
			<input type="text" model:value="{{ownerIdCard}}" style="width: 100%;font-size: 14px;color:#666666;"  placeholder="请输入身份证件号码"/>
		</view>	 
	</view>	
	<view class="item-input-t">
		<view class="item-input-t-l">手机号码</view>
		<view class="item-input-t-m">
			<input type="text" model:value="{{applicantPhone}}" style="width: 100%;font-size: 14px;color:#666666;"  placeholder="请输入手机号码"/>
		</view>	 
	</view>		 	 
</view>

<view class="btn" ><view   class="btn1" bindtap="formModal">下一步</view></view>
<view class="btn"><view class="btn2" bindtap="oshowModal">取消</view></view>
 <view  class='covers' wx:if="{{showModal}}">
	 <view class="covers-a">
	      <view class="covers-a-title">申请尚未提交，您确定退出吗？</view>
		  <block >			  
			    <view style="width: 80%;margin:0 auto;">
					<button  class="bt2"  bindtap="cshowModal">取消</button></view>
				<view style="width: 80%;margin:10px auto;">
					<button class="bt3"  bindtap="coshowModal">确定</button></view>
		  </block>	  
	 </view>	 
	 <view catchtap="hideCover"  class='bg' wx:if="{{showModal}}"></view>
 </view>
 
 <view style="position:fixed; bottom:0;width:100%;height:300px;background-color:#fff;border-top-left-radius: 6px;border-top-right-radius: 6px;z-index: 5;" wx:if="{{showd}}">
  	  <view style="padding: 15px;">
  		  <view style="display: flex;justify-content:flex-end;">
  		  		<view style="width:70%;font-size: 14.5px;color:#343434">请选择身份证件类型</view>
  		  		<view bindtap="cloBtnb">
  					<image src="/images/close.png" style="width: 14px;height: 14px;"/>
  				</view>
  		  </view>
  		  <view style="font-size: 16.5px;line-height: 40px;color:#343434;" wx:for="{{itemsshowd}}" id='{{index}}' bindtap="selBtnb">
  			  {{item.message}}
  		  </view>
  	  </view>	 
 </view>
 <view catchtap="hideCover"  class='bg' wx:if="{{isbg}}"></view>