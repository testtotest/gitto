  .covers {
           width: 100%;
           height: 100%;
           position: fixed;
           top: 0;
           left: 0;
           display: flex;
           justify-content: center;
           align-items: center;
           z-index: 6;
         }
		 .covers-a{
			 width: 600rpx;
			 height: 400rpx;
			 background: rgba(255, 255, 255, 1);
			 border-radius: 20rpx;
			 display: flex;
			 flex-direction: column;
			 z-index: 5;
		 }
		  .covers-a-title{
			 width:100%;
			 display: flex;			  
			 align-items: center;			  
			 justify-content: center;					 
			 margin-top: 82rpx;			
			 height: 32rpx;
			 font-size: 15px;			
			 color: #333333;
			 line-height: 36rpx;
			 margin-bottom: 62rpx;
			 /* font-weight: bold;  white-space: nowrap;	*/
		 }
   .bg {
           position: fixed;
           top: 0;
           left: 0;
           width: 100%;
           height: 100%;
           background: rgba(0, 0, 0, 0.5);
         }
         .cross {
           margin-bottom: 110rpx;
           bottom: 0rpx;
           position: fixed;
           width: 60rpx;
           height: 60rpx;
           z-index: 5;
         }
		 
		 
