	 <view style="position:fixed; bottom:0;width:100%;height:300px;background-color:#fff;border-top-left-radius: 6px;border-top-right-radius: 6px;z-index: 5;" wx:if="{{showe}}">
	  	  <view style="padding: 15px;">
	  		  <view style="display: flex;justify-content:flex-end;">
	  		  		<view style="width:70%;font-size: 14.5px;color:#343434">企业主体证明类型</view>
