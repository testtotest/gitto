	  				</view>
	  		  </view>
	  		  <view style="font-size: 16.5px;line-height: 40px;color:#343434;" wx:for="{{items}}" id='{{index}}' bindtap="selBtnb">
	  			  {{item.message}}
	  		  </view>
	  	  </view>	 
	 </view>
	 
	 <view style="position:fixed; bottom:0;width:100%;height:330px;background-color:#fff;border-top-left-radius: 6px;border-top-right-radius: 6px;z-index: 5;" wx:if="{{showa}}">
