 <view class="d-j">
	 	<view class="w-30 st2"></view>
	 	<view class="w-30 st1"></view>	
	 	<view class="w-30 st2"></view>
	 </view>
	 <view class="w-100 bg-f mt-10">
	 		  <view class="item-input-t item-input-t-border" bindtap="onBtnb">
	 		  	<view class="item-inputs-t-l">企业主体证明类型</view>
	 		  	<view class="item-inputs-t-m">{{isshowe}}</view>
	 		   	<view class="item-inputs-t-r"><image src="/images/gengduo@2x.png" style="width: 30rpx;height: 30rpx;"></image></view>
	 		  </view>
			  <view  class="picup">
			  	   <view class="picup-title">
			  	   	 		  <label class="picup-title-lables">最多可上传1张照片</label>
			  	   	 		  <label class="picup-title-lable"></label>
			  	   </view>
				    <view class="picup-list">
						<view wx:if="{{imgcc}}" class="picup-list-l">
						<image src="{{imgcc}}" style="width: 100%;height: 67px;"/>
						</view>
						<view class="picup-but" bindtap="choosecImg">
										    <view class="picup-but-pic">
												<image src="/images/xiangji@2x.png"/>
											</view>
											<view class="picup-but-btn">点击拍摄/上传</view>
						</view>	 		   
					</view>
			  </view>
	 </view>
	 <view class="w-100 bg-f mt-10">
		 <view class="item-input-t item-input-t-border">
		 				<view class="item-input-t-l">企业名称</view>
		 				<view class="item-input-t-m">
		 					<input model:value="{{enterpriseName}}" type="text" style="width: 100%;font-size: 14px;color:#666666;"  placeholder="请输入企业名称"/>
		 				</view>	 
		 </view>
		 <view class="item-input-t item-input-t-border" bindtap="onBtna">
		 	<view class="item-inputs-t-l">产权证明类型</view>
		 	<view class="item-inputs-t-m">{{isshowa}}</view>
		  	<view class="item-inputs-t-r"><image src="/images/gengduo@2x.png" style="width: 30rpx;height: 30rpx;"></image></view>
		 </view>
		 <view  class="picup">
		 	   <view class="picup-title">
		 	   	 		  <label class="picup-title-lables">最多可上传6张照片</label>
		 	   	 		  <label class="picup-title-lable"></label>
		 	   </view>
		 				    <view class="picup-list">
		 						<view wx:for="{{imgzz}}" wx:for-item="item" wx:key="*this" class="picup-list-l">
		 						<image  data-index="{{index}}" catchtap="deleteImg" src="{{item.photo}}" style="width: 100%;height: 67px;"/>								
		 						</view>
		 						<view class="picup-but" bindtap="choosezImg">
		 										    <view class="picup-but-pic">
		 												<image src="/images/xiangji@2x.png"/>
		 											</view>
		 											<view class="picup-but-btn">点击拍摄/上传</view>
		 						</view>	 		   
		 					</view>
		 </view>		
	 </view>	 
	 <view class="bt1">
	 			 <view  class="btn" bindtap="btnForm">下一步</view>
	 </view>
	 <view style="position:fixed; bottom:0;width:100%;height:300px;background-color:#fff;border-top-left-radius: 6px;border-top-right-radius: 6px;z-index: 5;" wx:if="{{showe}}">
	  	  <view style="padding: 15px;">
	  		  <view style="display: flex;justify-content:flex-end;">
	  		  		<view style="width:70%;font-size: 14.5px;color:#343434">企业主体证明类型</view>
	  		  		<view bindtap="cloBtnb">
	  					<image src="/images/close.png" style="width: 14px;height: 14px;"/>
	  				</view>
	  		  </view>
	  		  <view style="font-size: 16.5px;line-height: 40px;color:#343434;" wx:for="{{items}}" id='{{index}}' bindtap="selBtnb">{{item.message}}</view>
	  	  </view>	 
	 </view>
	 
	 <view style="position:fixed; bottom:0;width:100%;height:330px;background-color:#fff;border-top-left-radius: 6px;border-top-right-radius: 6px;z-index: 5;" wx:if="{{showa}}">
	  	  <view style="padding: 15px;">
	  		  <view style="display: flex;justify-content:flex-end;">
	  		  		<view style="width:70%;font-size: 14.5px;color:#343434">企业主体证明类型</view>
	  		  		<view bindtap="cloBtna">
	  					<image src="/images/close.png" style="width: 14px;height: 14px;"/>
	  				</view>
	  		  </view>
	  		  <view style="font-size: 16.5px;line-height: 40px;color:#343434;" wx:for="{{itemsa}}" id='{{index}}' bindtap="selBtna">
	  			  {{item.message}}
	  		  </view>
	  	  </view>	 
	 </view>
	 <view catchtap="hideCover"  class='bg' wx:if="{{isbg}}"></view>