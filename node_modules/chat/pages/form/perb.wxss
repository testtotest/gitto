/* pages/form/pera.wxss */
.bg-f{background-color: #ffff;}
.d-j{display:flex;justify-content:space-between;}
.w-100{width:100%;}
.w-50{width:47%;}
.st1{border: 2px solid #38CF91;border-radius: 5rpx;}
.st2{border: 2px solid #E3E4E6;border-radius: 5rpx;}
.mb-25{margin-bottom: 25px;}
.mt-10{margin-top: 10px;}

.btn{width:90%;margin:10px auto;}
.btn1{background-color: #38CF91;font-weight: 100;font-size: 17px;height: 42px;line-height: 42px;border-radius: 50rpx;}
.btn2{background-color: #F6F7F9;font-weight: 100;font-size: 15px;height: 42px;line-height: 42px;border-radius: 50rpx;color: #9A9A9A;}
.bt2{background-color: #38CF91;font-weight: 100;font-size: 14px;height: 40px;line-height: 40px;border-radius: 40rpx;}
.bt3{width: 50%;color: #999999;font-weight: 100;font-size: 14px;height: 40px;line-height: 40px;border-radius: 40rpx;}

.item-input-t{
	width: 90%;margin:0 auto;display:flex;justify-content:space-between;height: 50px;line-height: 50px;
}
.item-input-t-border{border-bottom: 0.5px solid #EDEDED;}
.item-input-t-l{width: 30%;color:#343434;font-size: 14px;}
.item-input-t-m{width: 65%;color:#666666;font-size: 14px;display: flex;justify-content:flex-end;align-items: center;padding-right: 10px;}
.item-input-t-r{width: 5%;}


.item-inputs-t-l{width: 42%;color:#343434;font-size: 14px;}
.item-inputs-t-m{width: 53%;color:#666666;font-size: 14px;display: flex;justify-content:flex-end;align-items: center;padding-right: 10px;}

.bg 
{
           position: fixed;
           top: 0;
           left: 0;
           width: 100%;
           height: 100%;
           background: rgba(0, 0, 0, 0.5);
}
.address-c {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 880rpx;
    border-radius: 12rpx 12rpx 0rpx 0rpx;
    background: #FFFFFF;
    animation-duration: 0.5s
}
.closed-btn{
    width: 26rpx;
    height: 26rpx;
}
.address-c .address-title{
    height: 85rpx;
    line-height: 85rpx;
    text-align: center;
    font-size: 32rpx;
    font-weight: 500;
    color: #343434;
}
.address-c .address-tab{
    padding-left: 30rpx;
    height: 85rpx;
    line-height: 85rpx;
    border-bottom: 1px solid #DEDEDE;
}
.address-tab text{
    display: inline-block;
    font-size: 32rpx;
    color: #343434;
    line-height: 85rpx;
    margin-right: 20rpx;
}
.address-tab text:last-child{
    color: #38D092;
    border-bottom: 1rpx solid #38D092;
}

.address-c .address-list{
    padding-top: 20rpx;
}

.address-c .address-list .address-item{
    height: 70rpx;
    line-height: 70rpx;
    padding-left: 30rpx;
    font-size: 30rpx;
    color: #343434;
}
.covers {
           width: 100%;
           height: 100%;
           position: fixed;
           top: 0;
           left: 0;
           display: flex;
           justify-content: center;
           align-items: center;
           z-index: 6;
}
.covers-a{
		width: 600rpx;
		height: 400rpx;
		background: rgba(255, 255, 255, 1);
		border-radius: 20rpx;
		display: flex;
		flex-direction: column;
		z-index: 5;
}
.covers-a-title{
	width:100%;
	display: flex;			  
	align-items: center;			  
	justify-content: center;					 
	margin-top: 82rpx;			
	height: 32rpx;
	font-size: 15px;			
	color: #333333;
	line-height: 36rpx;
	margin-bottom: 62rpx;
			 /* font-weight: bold;  white-space: nowrap;	*/
}


.picup{	padding:15px;}
.picup-title{padding-bottom: 15px;}
.picup-title-lable{font-size: 14.5px;color:#343434;}
.picup-title-lables{font-size: 12.5px;color:#9A9A9A;padding-left: 10px;}
.picup-list{display:flex;justify-content:space-between;}
.picup-list-l{width: 30%;height: 60px;border: 0.5px solid #E3E4E6;border-radius: 4px;background-color: #F6F7F9;}
.picup-but{width: 30%;height: 60px;border: 0.5px solid #E3E4E6;border-radius: 4px;background-color: #F6F7F9;padding-top: 7px;}
.picup-but-pic{display:flex;justify-content: center;}
.picup-but-pic image{width: 36px;height: 36px;}
.picup-but-btn{font-size: 11.5px;color: #9A9A9A;display:flex;justify-content: center;padding-top: 2px;}